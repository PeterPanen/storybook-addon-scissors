{
  "name": "storybook-addon-scissors",
  "version": "2.0.4",
  "description": "Addon for React Storybook providing responsive containers for stories",
  "main": "build/register.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prebuild": "rimraf build/*",
    "build": "babel src --out-dir build --copy-files"
  },
  "keywords": [
    "React",
    "Storybook",
    "Responsive",
    "Scissors",
    "Devices"
  ],
  "author": "Rasmus Knudsen",
  "license": "ISC",
  "repository": "https://github.com/PeterPanen/storybook-addon-scissors.git",
  "devDependencies": {
    "@storybook/react": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "node-fetch": "^1.7.1",
    "react-toggle": "^4.0.1",
    "react-virtualized-select": "^3.0.1"
  }
}
